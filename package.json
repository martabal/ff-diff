{
  "name": "ff-diff",
  "version": "0.2.7",
  "license": "MIT",
  "description": "Compare changes in user.js preference files between two versions of Firefox",
  "author": "martabal",
  "bin": {
    "ff-diff": "./dist/index.js"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martabal/ff-diff.git"
  },
  "keywords": [
    "firefox",
    "diff",
    "versions",
    "user:config",
    "user.js",
    "user_pref"
  ],
  "scripts": {
    "build": "MINIFY=true vite build",
    "check:typescript": "tsc --noEmit",
    "check:all": "npm run check:typescript && npm run test && npm run format && npm run lint",
    "ff-diff": "vite build -l warn && USE_CURRENT_DIR=true node dist/index.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "generate:usage": "USAGE=true vite build && node dist/usage.js",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix --fix-suggestions",
    "test": "vitest --run",
    "test:cov": "vitest --coverage",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@types/node": "24.1.0",
    "@types/selenium-webdriver": "4.1.28",
    "@vitest/coverage-v8": "3.2.4",
    "oxlint": "1.8.0",
    "prettier": "3.6.2",
    "typescript": "5.8.3",
    "vite": "7.0.5",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "selenium-webdriver": "4.34.0"
  },
  "engines": {
    "node": ">=20"
  },
  "type": "module"
}
